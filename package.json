{
  "name": "@vallme2003/cypress-ct-marko-js",
  "version": "0.0.0",
  "description": "Framework Definition for Marko.js with Cypress Component Testing",
  "exports": {
    "node": "./definition.js",
    "default": "./index.js"
  },
  "scripts": {
    "build": "npm run build:definition && npm run build:mount",
    "build:mount": "tsc --project tsconfig.mount.json",
    "build:definition": "tsc --project tsconfig.definition.json"
  },
  "types": "*.d.ts",
  "files": [
    "package.json",
    "definition.js",
    "index.js",
    "definition.d.ts",
    "index.d.ts"
  ],
  "devDependencies": {
    "cypress": "^13.3.1",
    "marko": "^5.31.11",
    "@marko/webpack": "^9.3.3",
    "typescript": "^4.9.5",
    "@types/marko": "^4.6.4"
  },
  "dependencies": {
    "@cypress/mount-utils": "^4.0.0"
  }
}
